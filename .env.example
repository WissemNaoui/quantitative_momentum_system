# API Keys (NEVER commit the actual .env file!)
# Copy this to .env and fill in your actual keys

# FinancialDatasets.ai API Key
FD_API_KEY=your_fd_api_key_here

# Finviz Elite API Token (optional)
FINVIZ_API_TOKEN=your_finviz_token_here

# MLflow Tracking URI (default: local SQLite)
MLFLOW_TRACKING_URI=sqlite:///mlflow.db

# DVC Remote Storage (optional)
# DVC_REMOTE_URL=s3://your-bucket/momentum-system
# AWS_ACCESS_KEY_ID=your_aws_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret

# Docker Hub (for CI/CD)
# DOCKER_USERNAME=your_dockerhub_username
# DOCKER_PASSWORD=your_dockerhub_password
